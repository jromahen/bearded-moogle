<!DOCTYPE look-and-feel PUBLIC
	"-//Liferay//DTD Look and Feel 6.2.0//EN"
	"http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">
<look-and-feel>
  <compatibility>
    <version>6.2.0+</version>
  </compatibility>
  <theme id="victorian-chocolate" name="Victorian Chocolate Theme">
    <settings>
      <setting key="navigation-enabled" value="true" type="checkbox" configurable="true" />
      <setting key="breadcrumb-enabled" value="true" type="checkbox" configurable="true" />
      <setting key="heading-logo-location" value="centre" type="select" options="left,centre,right" configurable="true" />
    </settings>
  </theme>
</look-and-feel>
